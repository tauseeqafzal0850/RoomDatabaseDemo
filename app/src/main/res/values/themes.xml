<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TestCode" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="HeadingFornato">
        <item name="singleLine">true</item>
        <item name="fontFamily">@font/fonarto</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_16sdp</item>
    </style>

    <style name="regularTextStyle">
        <item name="singleLine">true</item>
        <item name="hintTextColor">@color/cardview_dark_background</item>
        <item name="fontFamily">@font/robotomedium</item>
        <item name="android:textColor">@color/purple_500</item>
        <item name="android:textSize">@dimen/_12sdp</item>
    </style>
</resources>